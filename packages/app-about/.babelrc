{
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    "@babel/plugin-proposal-optional-chaining",
    "dynamic-import-node"
  ],
  "presets": ["next/babel"],
  "env": {
    "dev": {
      "plugins": [
        "babel-plugin-styled-components",
      ]
    },
    "production": {
      "plugins": [
        "babel-plugin-styled-components",
      ]
    },
    "test": {
      "plugins": [
        "babel-plugin-styled-components",
      ],
      "presets": [
        [
          "next/babel", {
            "transform-runtime": {
              "useESModules": false
            }
          }
        ]
      ]
    }
  }
}
