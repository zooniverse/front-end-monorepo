{
  "plugins": [
    "transform-decorators-legacy",
    "transform-class-properties",
    "transform-object-rest-spread"
  ],
  "presets": [
    "react",
    ["env", {
      "modules": false,
      "targets": {
        "browsers": "last 2 versions"
      }
    }]
  ],
  "env": {
    "test": {
      "presets": [
        "env",
        "react"
      ]
    }
  }
}
