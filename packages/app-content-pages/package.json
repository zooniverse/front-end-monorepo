    "next": "~8.0.1",
    "panoptes-client": "~2.12.0",
    "path-match": "~1.2.4",
    "react": "~16.8.4",
    "react-dom": "~16.8.4",
    "require-control": "~2.1.1",
    "styled-components": "~4.2.0",
    "url-parse": "~1.4.4",
    "validator": "~10.11.0"
  },
  "devDependencies": {
    "@babel/register": "~7.0.0",
    "@storybook/react": "~4.1.12",
    "babel-eslint": "~10.0.1",
    "chai": "~4.2.0",
    "dirty-chai": "^2.0.1",
    "enzyme": "~3.9.0",
    "enzyme-adapter-react-16": "~1.11.2",
    "jsdom": "~13.0.0",
    "mocha": "~5.2.0",
    "proxyquire": "^2.1.0",
    "sinon": "~7.2.3",
    "sinon-chai": "~3.3.0",
    "snazzy": "~8.0.0",
    "standard": "~12.0.1"
  },
  "engines": {
    "node": ">=10"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "globals": [
      "expect"
    ],
    "parser": "babel-eslint"
  }
}
