{
  "plugins": [],
  "presets": [
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ],
    [
      "@babel/preset-env",
      {
        "modules": "auto"
      }
    ]
  ],
  "env": {
    "es6": {
      "presets": [
        [
          "@babel/preset-react",
          {
            "runtime": "automatic"
          }
        ],
        [
          "@babel/preset-env",
          {
            "modules": false,
            "targets": {
              "esmodules": true
            }
          }
        ]
      ]
    }
  }
}
