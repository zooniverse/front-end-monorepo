{
  "name": "@zooniverse/react-components",
  "version": "0.7.1",
  "description": "Zooniverse React Components",
  "author": "Zooniverse",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -rf lib && npm run build-js && npm run build-css",
    "build-js": "babel src --out-dir lib",
    "build-css": "stylus src/css/zooniverse-react-components.styl --out lib",
    "lint": "eslint src",
    "postversion": "git push && git push --tags",
    "preversion": "npm test",
    "start": "echo 'Nothing to see here yet'",
    "test": "mocha --reporter nyan --require babel-core/register test/setup.js test/*.test.js",
    "version": "npm run build && git add -A lib"
  },
  "dependencies": {
    "animated-scrollto": "^1.1.0",
    "data-uri-to-blob": "0.0.4",
    "grommet": "^2.0.0-beta.3",
    "grommet-icons": "^1.0.0",
    "markdownz": "^7.6.0",
    "modal-form": "^2.6.0",
    "panoptes-client": "^2.8.1",
    "prop-types": "~15.6.0",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-select": "^1.0.0-rc.10",
    "react-swipe": "^5.1.1",
    "styled-components": "^3.2.6"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^8.2.3",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^4.1.2",
    "dirty-chai": "^2.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "jsdom": "^11.8.0",
    "mocha": "^5.1.1",
    "sinon": "^4.5.0",
    "stylus": "^0.54.5"
  },
  "engines": {
    "node": "8",
    "npm": "5"
  }
}
