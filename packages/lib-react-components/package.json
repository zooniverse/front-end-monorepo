{
  "name": "@zooniverse/react-components",
  "version": "0.7.1",
  "description": "Zooniverse React Components",
  "author": "Zooniverse",
  "license": "Apache-2.0",
  "main": "src/index.js",
  "scripts": {
    "build": "rm -rf lib && npm run build-js",
    "build-js": "babel src --out-dir lib --source-maps --copy-files --ignore spec.js,test.js,test.js.mocha-snapshot,spec.js.mocha-snapshot",
    "postversion": "git push && git push --tags",
    "preversion": "npm test",
    "start": "echo 'Nothing to see here yet'",
    "test": "mocha test/setup.js $(find src -name *.spec.js)",
    "test-updated-snapshots": "UPDATE=1 npm test",
    "version": "npm run build && git add -A lib",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@zooniverse/grommet-theme": "^2.0.0",
    "counterpart": "^0.18.6",
    "grommet": "^2.0.0-beta.5",
    "grommet-icons": "^2.0.0",
    "prop-types": "~15.6.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "styled-components": "^3.2.6",
    "styled-theming": "^2.2.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.8",
    "@storybook/addon-backgrounds": "^3.4.8",
    "@storybook/addon-info": "^3.4.8",
    "@storybook/addon-knobs": "^3.4.8",
    "@storybook/addon-links": "^3.4.8",
    "@storybook/addon-notes": "^3.4.8",
    "@storybook/addon-viewport": "^3.4.8",
    "@storybook/addons": "^3.4.8",
    "@storybook/react": "^3.4.8",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.16.0",
    "babel-runtime": "^6.26.0",
    "chai": "^4.1.2",
    "dirty-chai": "^2.0.1",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "jsdom": "^11.8.0",
    "mocha": "^5.1.1",
    "mocha-snapshots": "^4.1.0",
    "react-test-renderer": "^16.4.2",
    "sinon": "^4.5.0",
    "styled-components-test-utils": "^0.4.1"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  }
}
