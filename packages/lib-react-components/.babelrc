{
  "plugins": [
    "babel-plugin-styled-components",
    ["transform-imports", {
      "lodash": {
        "transform": "lodash/${member}"
      }
    }]
  ],
  "presets": [
    ["@babel/preset-react", {
      "runtime": "automatic"
    }],
    ["@babel/preset-env", {
      "modules": false,
      "targets": {
        "esmodules": true
      }
    }]
  ],
  "env": {
    "test": {
      "presets": [
        ["@babel/preset-env", {
          "modules": "commonjs",
        }]
      ]
    }
  }
}
