{
  "name": "@zooniverse/fe-project",
  "description": "Zooniverse front end app handling projects and classification",
  "license": "Apache-2.0",
  "author": "Zooniverse <contact@zooniverse.org> (https://www.zooniverse.org/)",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "build": "APP_ENV=${APP_ENV:-development} NODE_ENV=${NODE_ENV:-production} PANOPTES_ENV=${PANOPTES_ENV:-production} next build",
    "dev": "APP_ENV=${APP_ENV:-development} PANOPTES_ENV=${PANOPTES_ENV:-staging} node server/server.js",
    "dev:inspect": "APP_ENV=${APP_ENV:-development} PANOPTES_ENV=${PANOPTES_ENV:-staging} NODE_OPTIONS='--inspect' node server/server.js",
    "lint": "next lint",
    "start": "NODE_ENV=${NODE_ENV:-production} PANOPTES_ENV=${PANOPTES_ENV:-production} node server/server.js",
    "test": "BABEL_ENV=test vitest run",
    "test:ci": "BABEL_ENV=test vitest run --silent='passed-only' --hideSkippedTests",
    "storybook": "storybook dev -p 9001",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@sentry/nextjs": "~10.27.0",
    "@sindresorhus/string-hash": "~1.2.0",
    "@zooniverse/async-states": "~0.1.0",
    "@zooniverse/classifier": "^0.0.1",
    "@zooniverse/grommet-theme": "~4.0.0",
    "@zooniverse/panoptes-js": "~1.0.0",
    "@zooniverse/react-components": "~1.15.0",
    "cookie": "~1.0.2",
    "engine.io-client": "~6.6.0",
    "express": "^5.2.0",
    "grommet": "~2.47.0",
    "grommet-icons": "~4.12.0",
    "i18next": "~24.2.2",
    "lodash": "~4.17.23",
    "mobx": "~6.13.5",
    "mobx-react": "~9.2.0",
    "mobx-state-tree": "~5.4.0",
    "morgan": "^1.10.0",
    "newrelic": "~13.3.2",
    "next": "~15.5.10",
    "next-i18next": "~15.4.0",
    "ol": "^10.7.0",
    "panoptes-client": "~5.6.0",
    "react": "~19.2.0",
    "react-dom": "~19.2.0",
    "react-i18next": "~14.1.3",
    "react-resize-detector": "~9.1.0",
    "styled-components": "~6.1.13",
    "swr": "~2.3.7",
    "validator": "~13.15.20"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "~9.1.16",
    "@storybook/nextjs": "~9.1.16",
    "@storybook/react": "~9.1.16",
    "@testing-library/dom": "~10.4.1",
    "@testing-library/react": "~16.3.0",
    "@testing-library/user-event": "~14.6.1",
    "@vitejs/plugin-react": "~4.4.1",
    "babel-plugin-styled-components": "~2.1.4",
    "eslint-config-next": "~14.2.26",
    "eslint-plugin-jsx-a11y": "~6.10.1",
    "jsdom": "~26.1.0",
    "msw": "~2.7.0",
    "msw-storybook-addon": "~2.0.3",
    "nock": "~13.5.1",
    "selfsigned": "~2.4.1",
    "sinon": "~21.0.0",
    "storybook": "~9.1.17",
    "storybook-react-i18next": "~4.0.11",
    "vitest": "~3.1.3"
  },
  "engines": {
    "node": ">=20.5"
  },
  "msw": {
    "workerDirectory": [
      "storybook-public"
    ]
  }
}
