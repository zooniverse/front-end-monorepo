{
  "name": "@zooniverse/fe-project",
  "description": "Zooniverse front end app handling projects and classification",
  "license": "Apache-2.0",
  "author": "Zooniverse <contact@zooniverse.org> (https://www.zooniverse.org/)",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "_check": "check-engines && check-dependencies",
    "_serve": "node server/server.js",
    "_test": "npm run _check && BABEL_ENV=test mocha",
    "build": "npm run _check && PANOPTES_ENV=production next build",
    "dev": "npm run _check && npm run _serve",
    "lint": "npm run _check && standard --fix | snazzy; exit 0",
    "start": "npm run _check && NODE_ENV=production npm run _serve",
    "test": "npm run _test && exit 0",
    "test:ci": "npm run _test -- --reporter=min",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "dependencies": {
    "@zooniverse/async-states": "~0.0.1",
    "@zooniverse/classifier": "^0.0.1",
    "@zooniverse/grommet-theme": "~2.0.0",
    "@zooniverse/panoptes-js": "~0.0.1",
    "@zooniverse/react-components": "~0.7.1",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "counterpart": "^0.18.6",
    "d3": "^5.7.0",
    "grommet": "~2.3.1",
    "grommet-icons": "~3.2.1",
    "lodash": "~4.17.11",
    "mobx": "~5.5.0",
    "mobx-react": "~5.2.8",
    "mobx-state-tree": "~3.5.0",
    "next": "^8.0.1",
    "panoptes-client": "^2.12.0",
    "path-match": "~1.2.4",
    "react": "~16.6.3",
    "react-dom": "~16.6.3",
    "sinon": "^7.2.3",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1",
    "styled-components": "~4.1.1",
    "url-parse": "~1.4.4",
    "validator": "~10.11.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "~7.1.6",
    "@babel/plugin-proposal-optional-chaining": "~7.2.0",
    "@babel/register": "~7.0.0",
    "@storybook/react": "^4.1.12",
    "babel-eslint": "^10.0.1",
    "babel-plugin-styled-components": "~1.10.0",
    "chai": "~4.2.0",
    "check-dependencies": "~1.1.0",
    "check-engines": "~1.5.0",
    "dirty-chai": "^2.0.1",
    "enzyme": "~3.7.0",
    "enzyme-adapter-react-16": "~1.7.0",
    "jsdom": "~13.0.0",
    "mocha": "~5.2.0",
    "sinon-chai": "^3.3.0"
  },
  "engines": {
    "node": ">=8"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "globals": [
      "expect"
    ],
    "parser": "babel-eslint"
  }
}
