{
  "name": "@zooniverse/user",
  "description": "A library for Zooniverse user and user group pages",
  "license": "Apache-2.0",
  "author": "Zooniverse <contact@zooniverse.org> (https://www.zooniverse.org/)",
  "version": "0.0.0",
  "main": "dist/cjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./*": {
      "import": "./dist/esm/*/index.js",
      "require": "./dist/cjs/*/index.js"
    }
  },
  "sideEffects": false,
  "type": "module",
  "repository": "https://github.com/zooniverse/front-end-monorepo.git",
  "bugs": "https://github.com/zooniverse/front-end-monorepo/issues",
  "scripts": {
    "build": "yarn build:cjs && yarn build:es6",
    "build:cjs": "babel ./src/ --out-dir ./dist/cjs --copy-files --no-copy-ignored",
    "build:es6": "BABEL_ENV=es6 babel ./src/ --out-dir ./dist/esm --copy-files --no-copy-ignored",
    "dev": "BABEL_ENV=es6 webpack serve --config webpack.dev.js",
    "eslint": "eslint ./src --fix | snazzy",
    "prepare": "yarn build",
    "test": "BABEL_ENV=es6 mocha --loader=@node-loader/babel --config ./test/.mocharc.json \"./src/**/*.spec.js\"",
    "test:ci": "echo \"No CI tests yet...\" && exit 0",
    "storybook": "storybook dev -p 6008",
    "build-storybook": "echo \"No storybook to build yet...\" && exit 0"
  },
  "dependencies": {},
  "peerDependencies": {
    "@zooniverse/grommet-theme": "3.x.x",
    "@zooniverse/panoptes-js": "~0.4.0",
    "@zooniverse/react-components": "~1.x.x",
    "grommet": "2.x.x",
    "react": "~18.2.0",
    "react-dom": "~18.2.0"
  },
  "devDependencies": {
    "@babel/cli": "~7.23.0",
    "@babel/core": "~7.23.2",
    "@babel/eslint-parser": "~7.23.3",
    "@babel/plugin-transform-runtime": "~7.23.2",
    "@babel/preset-env": "~7.23.2",
    "@babel/preset-react": "~7.23.3",
    "@babel/register": "~7.22.5",
    "@node-loader/babel": "~2.0.1",
    "@storybook/addon-a11y": "~7.5.2",
    "@storybook/addon-essentials": "~7.5.2",
    "@storybook/react": "~7.5.2",
    "@storybook/react-webpack5": "~7.5.2",
    "@testing-library/dom": "~9.3.0",
    "@testing-library/react": "~14.1.0",
    "@testing-library/user-event": "~14.5.0",
    "babel-loader": "~9.1.0",
    "babel-plugin-module-resolver": "~5.0.0",
    "babel-plugin-transform-imports": "~2.0.0",
    "chai": "~4.3.4",
    "chai-dom": "~1.12.0",
    "css-loader": "~6.8.1",
    "dirty-chai": "~2.0.1",
    "eslint-plugin-jsx-a11y": "~6.8.0",
    "eslint-plugin-react": "~7.33.0",
    "html-webpack-plugin": "~5.5.0",
    "ignore-styles": "~5.0.1",
    "jsdom": "~23.0.0",
    "mocha": "~10.2.0",
    "nock": "~13.4.0",
    "panoptes-client": "~5.5.1",
    "process": "~0.11.10",
    "prop-types": "^15.8.1",
    "sinon": "~17.0.0",
    "sinon-chai": "~3.7.0",
    "snazzy": "~9.0.0",
    "storybook": "~7.5.2",
    "style-loader": "~3.3.1",
    "webpack": "~5.89.0",
    "webpack-cli": "~5.1.0",
    "webpack-dev-server": "~4.15.0"
  },
  "engines": {
    "node": ">=20.5"
  }
}
