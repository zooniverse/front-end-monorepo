{
  "name": "@zooniverse/user",
  "description": "Zooniverse library for user and user group pages",
  "license": "Apache-2.0",
  "author": "Zooniverse <contact@zooniverse.org> (https://www.zooniverse.org/)",
  "version": "0.1.0",
  "main": "dist/esm/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js"
    },
    "./*": {
      "import": "./dist/esm/*/index.js"
    }
  },
  "sideEffects": false,
  "repository": "https://github.com/zooniverse/front-end-monorepo.git",
  "bugs": "https://github.com/zooniverse/front-end-monorepo/issues",
  "scripts": {
    "build": "pnpm run build:es6",
    "build:es6": "BABEL_ENV=es6 babel ./src/ --out-dir ./dist/esm --copy-files --no-copy-ignored --ignore 'src/**/*.spec.js' --ignore 'src/**/*.spec.jsx' --ignore 'src/**/*.stories.jsx'",
    "eslint": "eslint ./src --fix | snazzy",
    "prepare": "pnpm run build",
    "test": "vitest run",
    "test:ci": "vitest run --silent='passed-only' --hideSkippedTests",
    "coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6008 ",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@zooniverse/async-states": "~0.1.0",
    "@zooniverse/content": "~0.0.1",
    "@zooniverse/panoptes-js": "~1.0.0",
    "dayjs": "~1.11.11",
    "i18next": "~24.2.2",
    "panoptes-client": "~5.6.0",
    "react-i18next": "~14.1.3",
    "swr": "~2.3.2",
    "uuid": "~11.1.0"
  },
  "peerDependencies": {
    "@zooniverse/grommet-theme": "4.x.x",
    "@zooniverse/react-components": "~1.x.x",
    "grommet": "2.x.x",
    "grommet-icons": "~4.x.x",
    "next": "~14.2.26",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "styled-components": "6.x.x"
  },
  "devDependencies": {
    "@babel/eslint-parser": "~7.28.4",
    "@babel/plugin-transform-runtime": "~7.28.3",
    "@babel/preset-env": "~7.28.3",
    "@babel/preset-react": "~7.27.1",
    "@storybook/addon-a11y": "~7.6.11",
    "@storybook/addon-essentials": "~7.6.11",
    "@storybook/react": "~7.6.11",
    "@storybook/react-webpack5": "~7.6.11",
    "@testing-library/dom": "~9.3.0",
    "@testing-library/react": "~14.2.0",
    "@testing-library/user-event": "~14.5.0",
    "@vitejs/plugin-react": "~4.4.1",
    "@zooniverse/grommet-theme": "~4.0.0",
    "@zooniverse/react-components": "~1.15.0",
    "babel-plugin-module-resolver": "~5.0.0",
    "babel-plugin-styled-components": "~2.1.4",
    "babel-plugin-transform-imports": "~2.0.0",
    "eslint-plugin-jsx-a11y": "~6.10.1",
    "eslint-plugin-react": "~7.37.2",
    "jsdom": "~26.1.0",
    "msw": "~2.7.0",
    "msw-storybook-addon": "~2.0.3",
    "nock": "~13.5.4",
    "prop-types": "^15.8.1",
    "sinon": "~21.0.0",
    "snazzy": "~9.0.0",
    "storybook": "~7.6.11",
    "vitest": "~3.1.3"
  },
  "msw": {
    "workerDirectory": "./storybook-public"
  },
  "engines": {
    "node": ">=20.5"
  }
}
